#!/bin/bash
set -e
export PASSPHRASE="default"
if [ -z "$PASSPHRASE" ]; then
  echo "No PASSPHRASE environment variable"
  exit 1
fi

if [ -z "$COINS" ]; then
  source /Volumes/Data/workspaces/komodo/electron/chentsulin-electron-react-boilerplate/app/bin/marketmaker/coins
  COINS=$coins
fi
USERHOME="/Volumes/Data/workspaces/komodo/electron/chentsulin-electron-react-boilerplate/app/bin"
exec "/Volumes/Data/workspaces/komodo/electron/chentsulin-electron-react-boilerplate/app/bin/marketmaker/OSX/marketmaker" "{\"gui\":\"dICOapp-cm\", \"client\":1, \"canbind\":0, \"docker\":1, \"userhome\":\"/${USERHOME#"/"}\", \"passphrase\":\"$PASSPHRASE\", \"coins\":$COINS}"